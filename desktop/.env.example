# TeamBeam Desktop Environment Configuration

# Supabase Configuration
# Get these values from your Supabase project dashboard
REACT_APP_SUPABASE_URL=https://your-project-id.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here

# Application Configuration
REACT_APP_APP_NAME=TeamBeam
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Features Flags
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_COLLABORATION=true
REACT_APP_ENABLE_OFFLINE_MODE=true

# Development Settings
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug

# File Upload Limits
REACT_APP_MAX_FILE_SIZE=100MB
REACT_APP_ALLOWED_FILE_TYPES=pdf,dwg,png,jpg,jpeg,gif,mp4,mov

# Integration Settings
REACT_APP_ENABLE_TEAMS_INTEGRATION=false
REACT_APP_TEAMS_CLIENT_ID=
REACT_APP_TEAMS_TENANT_ID=

# Analytics (optional)
REACT_APP_ANALYTICS_ID=
REACT_APP_SENTRY_DSN=